<!--index.wxml-->
<view class='custom-bar'>
  <le-custom-bar title="Le UI" show-navigator="{{false}}" type="{{1}}"></le-custom-bar>
</view>
<view class="page">
  <view class='top-area flex-center'>
    Le UI原生组件库
  </view>
  <view class='center flex-row'>
    <view class='list' wx:for="{{components}}" wx:key="{{index}}" data-page="{{item.page}}" catchtap='goPage'>
      <view class='title'>
        <le-icon name="{{item.icon}}" class='title-icon'></le-icon>
      </view>
      <view class='des'>{{item.name}}</view>
    </view>
  </view>
</view>