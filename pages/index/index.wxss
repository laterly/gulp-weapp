/**index.wxss**/

page{
   background-color: #fafafa;
}

.top-area {
  box-sizing: border-box;
  height: 140rpx;
  width: 100%;
  background: #1cbbb4;
  border-radius: 0px 0px 18px 18px;
  box-shadow: 0 1rpx 6rpx rgba(0, 0, 0, 0.1);
  color: #fff;
}

.center {
  padding: 0px 60rpx;
  display: flex;
  justify-content: space-between;
}

.list {
  padding: 40rpx 0;
  border-radius: 12rpx;
  width: 47%;
  margin-top: 40rpx;
  background-color:#fff;
  text-align: center;
  font-size: 14px;
  box-shadow:6rpx 6rpx 8rpx rgba(241,241,241, 0.2);
}
.list .title .title-icon text{
  font-size:50rpx;
  color: #1cbbb4;
}
.list .des{
  margin-top: 10px;
  color:#aaa;
}

